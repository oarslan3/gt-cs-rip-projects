  author =	 {Daniel Wagner and Dieter Schmalsteig},
  title =	 {ARToolKitPlus for pose tracking on mobile devices},
  booktitle =	 {Computer Vision Winter Workshop},
  year =	 {2007},
  c-alireza =	 {They present ARToolKitPlus in this paper, a
