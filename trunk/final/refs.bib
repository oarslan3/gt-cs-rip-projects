  title =	 {Preemptive {RANSAC} for Live Structure and Motion
                  Estimation},
  booktitle =	 ICCV,
  pages =	 {199-206},
  location =	 {Nice, France},
  month =	 {Oct},
  year =	 2003,
  abstract =	 {A system capable of performing robust live
